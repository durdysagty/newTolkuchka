@model List<string>
@{
    string cl = "col px-0 px-sm-2";
    string i = Model[1] + " fa-lg";
}
@if (Model.Count > 3)
{
    if (Model[3].Contains("#"))
    {
        <a data-mdb-toggle="modal" data-mdb-target="@Model[3]" href="/@Model[0]" class="@cl">
            <div>
                <i class="@i"></i>
            </div>
            <p>@Localizer[Model[2]]</p>
        </a>
    }
    else
    {
        cl = cl + " " + Model[4];
        <a data-target="@Model[3]" href="/@Model[0]" class="@cl">
            <div>
                <i class="@i"></i>
            </div>
            <p>@Localizer[Model[2]]</p>
        </a>
    }
}
else
{
    <a href="/@Model[0]" class="@cl">
        <div>
            <i class="@i"></i>
            @if (Model[0] == ConstantsService.CART)
            {
                <span name="q" class="badge dark badge-dark rounded-pill q2">0</span>
            }
            @if (Model[0] == ConstantsService.COMPARISON)
            {
                    <span name="s" class="badge dark badge-dark rounded-pill q2">0</span>
            }
        </div>
        <p>@Localizer[Model[2]]</p>
    </a>
}